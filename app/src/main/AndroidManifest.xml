<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.covid19helper">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".GlobalNewsActivity"></activity>
        <activity android:name=".CovidCasesActivity" />
        <activity android:name=".HistoryActivity" />
        <activity android:name=".SelfAssessmentActivity" />
        <activity android:name=".HelplineActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".HomeActivity" />
        <activity android:name=".HomeScreenActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".FPScreen3Activity" />
        <activity android:name=".FPScreen2Activity" />
        <activity android:name=".ForgotPassword" />
        <activity android:name=".Signup" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>